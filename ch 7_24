#include <cstdlib>
#include <iostream>
#include <cstring>
#include<fstream>
using namespace std;
int main() {
	int n = 0, i = 0, j;

	while (i != 3) {
		cout << "輸入一個介在1~50間的整數:";
		cin >> n;

		while (n < 1 || n>50) {
			cout << "輸入一個介在1~50間的整數";
			cin >> n;
		}

		j = 0;

		while (j != n) {
			cout << "*";
			j++;
		}

		i++;
	}
	return 0;
}
