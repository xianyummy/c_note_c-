#include <cstdlib>
#include <iostream>
#include <cstring>
#include<fstream>
using namespace std;
int main() {
	int num;

	cout << "輸入一個1~7的整數";
	cin >> num;

	switch (num) {
	case 1:
	case 2:
	case 3:
	case 4:
	case 5:
		cout << "今天要上班";
		break;
	case 6:
	case 7:
		cout << "今天要休息";
		break;
	default:
		cout << "輸入錯誤";
	}
	return 0;
}
