#include <cstdlib>
#include <iostream>
#include <string>
using namespace std;
int main() {
	int score, i = 0, a = 0, b = 0, c = 0;
	while (i != 10) {
		i++;
		cout << "輸入學生成績" << i << ":";
		cin >> score;

		if (score > 75) {
			a++;
		}
		else if (score > 59 && score < 76) {
			b++;
		}
		else {
			c++;
		}
	}
	cout << "A有" << a << "人" << endl;
	cout << "B有" << b << "人" << endl;
	cout << "C有" << c << "人" << endl;
	return 0;
}
